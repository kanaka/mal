["do",

["def", "_path", ["require", ["`", "path"]]],

["def", "_node_readline", ["require", [".", "_path", ["`", "resolve"],
                                            ["`", "."],
                                            ["`", "node_readline.js"]]]],

["def", "div", ["fn", ["a", "b"], ["parseInt", ["/", "a", "b"]]]],

["def", "time-ms", ["fn", [],
  [".", ["new", "Date"], ["`", "getTime"]]]],


["def", "assoc", ["fn", ["src-hm", "&", "kvs"],
  ["let", ["hm", ["clone", "src-hm"]],
    ["assocs!", "hm", "kvs"]]]],

["def", "dissoc", ["fn", ["src-hm", "&", "ks"],
  ["let", ["hm", ["clone", "src-hm"]],
    ["do",
      ["map", ["fn", ["k"], ["del", "hm", "k"]], "ks"],
      "hm"]]]],

["def", "_get", ["fn", ["obj", "key"],
  ["if", ["nil?", "obj"],
    null,
    ["if", ["contains?", "obj", "key"],
      ["get", "obj", "key"],
      null]]]],

["def", "_count", ["fn", ["a"],
  ["if", ["=", null, "a"],
    0,
    ["count", "a"]]]],

["def", "_nth", ["fn", ["seq", "idx"],
  ["if", [">=", "idx", ["count", "seq"]],
    ["throw", "nth: index out of range"],
    ["nth", "seq", "idx"]]]],

["def", "_first", ["fn", ["seq"],
  ["if", ["empty?", "seq"],
    null,
    ["first", "seq"]]]],

["def", "_rest", ["fn", ["seq"],
  ["if", ["empty?", "seq"],
    ["`", []],
    ["rest", "seq"]]]],

["def", "_apply", ["fn", ["f", "&", "args"],
  ["let", ["fn", ["if", ["malfunc?", "f"], ["get", "f", ["`", "fn"]], "f"],
           "fargs", ["concat", ["slice", "args", 0, ["-", ["count", "args"], 1]],
                               ["nth", "args", ["-", ["count", "args"], 1]]]],
    ["apply", "fn", "fargs"]]]],

["def", "_map", ["fn", ["f", "seq"],
  ["let", ["fn", ["if", ["malfunc?", "f"], ["get", "f", ["`", "fn"]], "f"]],
    ["map", "fn", "seq"]]]],

["def", "with_meta", ["fn", ["obj", "m"],
  ["let", ["new-obj", ["clone", "obj"]],
    ["do",
      ["set", "new-obj", ["`", "__meta__"], "m"],
      "new-obj"]]]],

["def", "meta", ["fn", ["obj"],
  ["if", ["or", ["sequential?", "obj"],
                ["map?", "obj"],
                ["malfunc?", "obj"]],
    ["if", ["contains?", "obj", ["`", "__meta__"]],
      ["get", "obj", ["`", "__meta__"]],
      null],
    null]]],

["def", "reset!", ["fn", ["atm", "val"],
  ["set", "atm", ["`", "val"], "val"]]],

["def", "swap!", ["fn", ["atm", "f", "&", "args"],
  ["let", ["fn", ["if", ["malfunc?", "f"], ["get", "f", ["`", "fn"]], "f"],
           "fargs", ["cons", ["get", "atm", ["`", "val"]], "args"],
           "val", ["apply", "fn", "fargs"]],
  ["do",
    ["set", "atm", ["`", "val"], "val"],
    "val"]]]],

["def", "core-ns",
