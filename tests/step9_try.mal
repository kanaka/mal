;;;(keyword :abc)
;;;;=>:abc
(keyword "abc")
;=>:abc

;; Testing sequential? function

(sequential? (list 1 2 3))
;=>true
(sequential? [15])
;=>true
(sequential? sequential?)
;=>false
(sequential? nil)
;=>false
(sequential? "abc")
;=>false

